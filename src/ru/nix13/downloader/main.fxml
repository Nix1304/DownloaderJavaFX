<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="427.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="ru.nix13.downloader.Controller">
    <children>
        <Pane prefHeight="355.0" prefWidth="600.0">
            <children>
                <TextArea fx:id="links" prefHeight="355.0" prefWidth="650.0" promptText="Enter links here..."/>
            </children>
        </Pane>
        <Pane prefHeight="68.0" prefWidth="650.0">
            <children>
                <Button layoutX="525.0" layoutY="6.0" mnemonicParsing="false" onAction="#download" prefHeight="27.0"
                        prefWidth="111.0" style="-fx-background-color: #3498db;" text="Start download"
                        textFill="WHITE"/>
                <TextField fx:id="save_name" layoutX="14.0" layoutY="21.0" prefHeight="25.0" prefWidth="357.0"
                           promptText="Save name..."/>
                <TextField fx:id="extension" layoutX="371.0" layoutY="21.0" prefHeight="27.0" prefWidth="145.0"
                           promptText="Extenstion"/>
                <Button layoutX="525.0" layoutY="41.0" mnemonicParsing="false" onAction="#openVKFunctions"
                        prefHeight="27.0" prefWidth="111.0" style="-fx-background-color: #3498db;" text="VK Functions"
                        textFill="WHITE"/>
            </children>
        </Pane>
    </children>
</VBox>
